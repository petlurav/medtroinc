from infra.component.component import Component

import tests.conf as conf


class ReportingSut(Component):

    def __init__(self):
        super().__init__()
        self.dss = conf.dss
        self.reporting = conf.reporting_cc
        self.bucket = conf.b3_s3
        self.bucket.name = "dss-dev-archive"
        self.bucket.table_name = 'dss_dev_data_store'
        self.dynamo = conf.dynamo
